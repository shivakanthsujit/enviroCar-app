<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/spacing_xxxlarge">


    <TextView
        android:id="@+id/activity_logbook_car_spinner_entry_firstline"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/roboto"
        android:text="Audi A3"
        android:textColor="#000000"
        android:textSize="@dimen/subtitle"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/activity_logbook_car_spinner_entry_secondline"
        app:layout_constraintEnd_toEndOf="@+id/activity_logbook_car_spinner_entry_secondline"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/activity_logbook_car_spinner_entry_secondline"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/activity_logbook_car_spinner_entry_secondline"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="sans-serif-light"
        android:text="2009 diesel"
        android:textSize="@dimen/text_small"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_logbook_car_spinner_entry_firstline" />


</androidx.constraintlayout.widget.ConstraintLayout>
